{
  "name": "unimodel-schema-types",
  "version": "0.2.7",
  "description": "Use unimodel documents as common-schema types",
  "main": "./dist/lib/index.js",
  "keywords": [
    "unimodel",
    "schema",
    "common-schema"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zipscene/unimodel-schema-types.git"
  },
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint . || exit 0",
    "docs": "node ./node_modules/.bin/yuidoc .",
    "test": "npm run-script build && node ./node_modules/.bin/mocha ./dist/test -R spec",
    "build": "rm -rf dist && ./node_modules/.bin/broccoli build dist && rm -rf tmp",
    "prepublish": "npm run-script build && npm run-script docs"
  },
  "dependencies": {
    "babel-runtime": "6.18.0",
    "common-query": "^1.10.3",
    "common-schema": "^1.8.0",
    "lodash": "^4.17.2",
    "xerror": "^1.1.1"
  },
  "devDependencies": {
    "broccoli": "^0.16.9",
    "broccoli-babel-transpiler": "^5.6.1",
    "broccoli-cli": "^1.0.0",
    "broccoli-merge-trees": "^1.1.4",
    "broccoli-static-compiler": "^0.2.2",
    "broccoli-wrap": "0.0.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "es6-promise": "^4.0.5",
    "eslint": "^3.10.2",
    "eslint-config-zipscene": "^0.1.11",
    "mocha": "^3.1.2",
    "source-map-support": "^0.4.6",
    "unimodel-core": "^0.6.14",
    "yuidocjs": "^0.10.2"
  },
  "license": "Apache-2.0"
}
